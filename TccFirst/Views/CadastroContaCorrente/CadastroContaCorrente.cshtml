div id="modal-cadastro-conta-corrente" class="modal fade" tabindex="-1" role="dialog">
<div class="modal-dialog" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">Cadastrar Conta Corrente</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <div class="form-group">
                <label for="cadastro-conta-corrente-campo-idAgencia">Nome</label>
                <select class="form-control" id="idAgencia"></select>
            </div>
            <div class="form-group">
                <label for="cadastro-conta-corrente-campo-numero-conta" mas>CPF</label>
                <input type="text" id="cadastro-conta-corrente-campo-numero-conta" class="form-control" />
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn" data-dismiss="modal"><i class="fa fa-close"></i> Cancelar</button>
            <button type="button" class="btn" id="cadastro-conta-corrente-botao-salvar"><i class="fa fa-save"></i> Salvar</button>
        </div>
    </div>
</div>