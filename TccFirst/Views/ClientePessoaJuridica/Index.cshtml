                <th>UF</th>
                <th>Cidade</th>
                <th>Ação</th>
            </tr>
    </thead>
</table>
@RenderPage("CadastroPessoaJuridica.cshtml")
    <link href="~/Content/css/ClientePessoaJuridica/TelaIndexPessoaJuridica.css" rel="stylesheet" />
