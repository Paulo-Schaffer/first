<div id="modal-historico" class="modal" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"><b>Cadastro de Histórico</b></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label for="historico-campo-descricao" mas><b>Descrição<span class="text-danger">*</span></b></label>
                    <input type="text" id="historico-campo-descricao" class="form-control" />
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-danger" id="botao-cancelar" data-dismiss="modal"><i class="fa fa-close"></i> Cancelar</button>
                <button type="button" class="btn btn-success" id="historico-botao-salvar"><i class="fa fa-floppy-o"></i> Salvar</button>
            </div>
        </div>
    </div>
</div>

<link rel="stylesheet" href="~/Content/css/Historico/TelaCadastroHistorico.css">
