@{
    ViewBag.Title = "RelatorioTituloReceber";
}


<head>
    <link rel="stylesheet" type="text/javascript" href="//cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css">
    <link rel="stylesheet" type="text/javascript" href="//cdn.datatables.net/buttons/1.5.6/css/buttons.dataTables.min.css">
</head>

<h2><b>Relatorio Titulos Receber</b></h2>
<hr />
<div class="form-group">
    <label for="filtro-receita"><b>Tipo de Receita</b><span class="text-danger">*</span></label>
    <select class="form-control border-dark" id="filtro-receita">
        <option value="0">Todos</option>
    </select>
</div>

<div class="row">
    <div class="col-6">
        <div class="form-group">
            <label for="filtro-documento"><b>Descrição</b></label>
            <input type="text" id="filtro-descricao" class="form-control border-dark" />
        </div>
    </div>
    <div class="col-6">
        <div clas="form-group">
            <label class="filtro-valor"><b>Valor</b></label>
            <input type="text" id="filtro-valor" class="form-control border-dark" />
        </div>
    </div>
</div>

<div class="row">
    <div class="col-6">
        <div clas="form-group">
            <label for="filtro-data-inicial"><b>Data Incial do Lançamento</b></label>
            <input type="date" id="filtro-data-inicial" class="form-control border-dark" />
        </div>
    </div>
    <div class="col-6">
        <div clas="form-group">
            <label for="filtro-data-final"><b>Data Final do Lançamento</b></label>
            <input type="date" id="filtro-data-final" class="form-control border-dark" />
        </div>
    </div>
</div>

<table id="relatorio-titulo-receber" class="table table-hover table-bordered table-striped">
    <thead>
        <tr>
            <th>Descrição</th>
            <th>Valor</th>
            <th>Data de Lançamento</th>
            <th>Tipo de Receita</th>

        </tr>
    </thead>
</table>
@section js{
    <script src="~/Scripts/Relatorio/relatorio-titulo-receber.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.5.6/js/dataTables.buttons.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.flash.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.html5.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.print.min.js"></script>
}


